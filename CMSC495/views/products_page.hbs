  <div class="container">

    <div class="row text-left justify-content-center">

      <div class="col-lg-8 col-md-3 mb-4">
        <h4 id="card-description-title">Description</h4>
        <p class="product-text">{{this.category_description}}</p>

        <h4 id="card-description-title">Technologies Used</h4>

        <div class="row">
          {{#if this.category_javascript}}
          <i class="fab fa-js-square"></i>
          {{/if}}

          {{#if this.category_react}}
          <i class="fab fa-react"></i>
          {{/if}}

          {{#if this.category_html}}
          <i class="fab fa-html5"></i>
          {{/if}}

          {{#if this.category_css}}
          <i class="fab fa-css3-alt"></i>
          {{/if}}

          {{#if this.category_python}}
          <i class="fab fa-python"></i>
          {{/if}}

          {{#if this.category_node}}
          <i class="fab fa-node-js"></i>
          {{/if}}

          {{#if this.category_express}}
          <i class="fab fa-js-square"></i>
          {{/if}}

          {{#if this.category_json}}

          {{/if}}

          {{#if this.category_rest}}

          {{/if}}

          {{#if this.category_sql}}
          <i class="fas fa-database"></i>
          {{/if}}

          {{#if this.category_cloud}}
          <i class="fas fa-cloud-upload-alt"></i>
          {{/if}}

        </div>

        {{#if this.category_git}}
        <h4 id="card-description-title">Project Repo</h2>
          <a class="product-text" href="{{this.category_git}}" target='blank'>{{this.category_git}}</a>
          {{/if}}
      </div>
      <div class="col-lg-4 col-md-3 mb-4">

        <h4 id="card-description-title">Team Name</h2>
          <p class="product-text">{{this.category_team_name}}</p>

          <h4 id="card-description-title">Members</h2>
            <p class="product-text">{{this.category_memberone}}</p>
            {{#if this.category_url}}
            <h4 id="card-description-title">URL</h2>
              <a class="product-text" href="{{this.category_url}}" target='blank'>{{this.category_url}}</a>
              {{/if}}
              <h4 id="card-description-title">Cohort</h2>
                <a href="https://www.digitalcrafts.com" target="blank">
                  <p class="product-text">{{this.category_cohort}}</p>
                </a>

      </div>

    </div>

