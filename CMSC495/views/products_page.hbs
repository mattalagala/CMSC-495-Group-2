          <h4 id="contact-description-title">Developer</h2>
            <p class="product-text">{{this.category_memberone}}</p>
            <div class="row">

            </div>
        </div>

        <div class="col-lg-4 col-md-3 mb-4">


          <h4 id="contact-description-title">Social</h2>


            {{#if this.category_onegit}}
            <a href="{{this.category_git}}" target="blank"><i class="fab fa-github-square" id="socialIcons"></i></a>
            {{/if}}

            {{#if this.category_onelinkedin}}
            <a href="{{this.category_onelinkedin}}" target="blank"><i class="fab fa-linkedin" id="socialIcons"></i></a>
            {{/if}}

            {{#if this.category_onetwitter}}
            <a href="{{this.category_twitter}}" target="blank"><i class="fab fa-twitter-square"
                id="socialIcons"></i></a>
            {{/if}}

            <h4 id="contact-description-title">Email</h2>
              <a href="mailto:{{this.category_twitter}}" target="blank">
                <p class="product-text">{{this.category_oneemail}}</p>
              </a>

              <a href="https://www.digitalcrafts.com" target="blank">
                <h4 id="contact-description-title">Cohort</h2>
                  <p class="product-text">{{this.category_cohort}}</p>
              </a>

        </div>




      </div>

    </div>



















